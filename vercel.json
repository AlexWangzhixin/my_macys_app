  "version": 2,
  "framework": "vite",
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/static",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://mymacys-f9v1s5qb1-alexs-projects-5c793e1a.vercel.app/$1",
      "permanent": true
    }
  ]
}